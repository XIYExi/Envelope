{
  "name": "envelope",
  "author": "xiye",
  "description": "lowcode design platform",
  "version": "2.0.0",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "start": "umi dev",
    "build": "cross-env umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "dumi": "umi dev --dumi ",
    "dumi build": "umi build --dumi",
    "build-storybook": "build-storybook",
    "electron": "electron --inspect=8000 -w ./",
    "start:electrondev": "cross-env NODE_ENV=development nodemon --watch ./main/ --exec \"electron ./main/main.js\"",
    "start:umidev": "cross-env UMI_DEV=dev umi dev",
    "electron start!": "concurrently \"npm run start:umidev\" \"wait-on http://localhost:8000 && npm run start:electrondev\"",
    "dist-win64": "cross-env ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ electron-builder -w --config ./electron-builder.config.js",
    "dist-linux": "cross-env ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ electron-builder -l --config ./electron-builder.config.js",
    "dist-mac": "cross-env ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ electron-builder -m --config ./electron-builder.config.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
